    "css-loader": "^0.23.1",
    "eslint": "^2.5.3",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-react": "^2.3.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^4.2.3",
    "eslint-plugin-standard": "^1.3.2",
    "hjs-webpack": "^8.0.0",
    "postcss-loader": "^0.8.2",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.2",
